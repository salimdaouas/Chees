package com.objis.chess;

import javax.swing.JButton;
import javax.swing.JFrame;
import javax.swing.JPanel;

public class Fenetre1 extends static JFrame{
	private static JButton bouton = new JButton("jouer");
	private static JPanel conteneur = new JPanel();
public Fenetre1() {
}
	public static void main(String[] args) {
		// TODO Instancier une fenetre
		JFrame maFenetre1 = new JFrame();
		// TODO Afficher la JFrame	
		maFenetre1.setTitle("Chess");
		maFenetre1.setSize(800,500);
		maFenetre1.setVisible(true);
		conteneur.add(bouton);
		setContentPane(conteneur);
		
	}

}
